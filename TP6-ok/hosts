[clients]
client ansible_host=<ip_client> ansible_user=root ansible_ssh_private_key_file=./ares role=client
[routers]
router ansible_host=<ip_router> ansible_user=root ansible_ssh_private_key_file=./ares role=router
[targets]
target ansible_host=<ip_target> ansible_user=root ansible_ssh_private_key_file=./ares role=target
